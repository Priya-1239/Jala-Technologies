rev = 0
while(n > 0):
    d = a % 10
    rev = rev * 10 + d
    a = a // 10
if(temp == rev):
    print("palindrome")
else:
    print("not a palindrome") 
'''
Output
 121
not a palindrome
'''

# Program to check whether a number is EVEN or ODD using switch
a=int(input())
if a%2==0:
    print("Even")
else:
    print("Odd")
'''
Output
 6
Even
'''

# Print gender (Male/Female) program according to given M/F using switch
a=input("Enter M or F to know the gender")
if a=="M" or a=="m":
    print("Male")
elif a=="F" or a=="f":
    print("Female")
else:
    print("Invalid Input")
'''
Output
 Enter M or F to know the gender F
Female
'''














